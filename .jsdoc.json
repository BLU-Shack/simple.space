{
    "opts": {
        "destination": "./docs/",
        "recurse": true,
        "template": "node_modules/tui-jsdoc-template"
    },
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": false
    },
    "templates": {
        "logo": {
            "name": "simple.space",
            "footerText": "A Botlist.Space Alternate Package"
        }
    }
}